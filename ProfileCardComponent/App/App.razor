<Router
	AppAssembly="@typeof(App).Assembly" >
	<Found
		Context="routeData" >
		<RouteView
			DefaultLayout="@typeof(MainLayout)"
			RouteData="@routeData" />
		<focusonnavigate
			RouteData="@routeData"
			Selector="h1" />
	</Found>
	<NotFound>
		<pagetitle>Not found</pagetitle>
		<LayoutView
			Layout="@typeof(MainLayout)" >
			<p
				role="alert" >
				Sorry, there's nothing at this address.
			</p>
		</LayoutView>
	</NotFound>
</Router>